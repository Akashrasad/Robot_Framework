                <LabelWidget var="var_action_bar_text_vc">
                    <ID>virtualcontrols_1_3_ah_cardLKA_csm</ID>
                </LabelWidget>
                <ImageWidget var="img_exit_btn">
                    <ID>img_lane_keep_assist_bzsa_only_close</ID>
