                        <DataValue>1</DataValue>
                        <StaticValue>virtualcontrols_1_3_7_g_BZSAOff_csm</StaticValue>
                        <Font var="var_font_radio_btn_off_normal"/>
                    </SelectorState>
                    <SelectorState>
                        <DataValue>2</DataValue>
                        <StaticValue>virtualcontrols_1_3_7_g_BZSAOff_csm</StaticValue>
                        <Font var="var_font_radio_btn_off_focused"/>
                    </SelectorState>
                </LabelWidget>
                <ImageWidget var="img_2ndrow_radio_multi_btn">
                    <ID>img_radio_button_on</ID>
					<NormalURL compression="CompressionAlias1">11/icn_radio_button_selected_large.png</NormalURL>
					<FocusURL compression = "CompressionAlias1">11/icn_radio_button_normal_large.png</FocusURL>                   
