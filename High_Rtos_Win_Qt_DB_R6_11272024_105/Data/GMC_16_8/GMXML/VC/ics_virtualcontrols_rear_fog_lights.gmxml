                        <WidgetCallback>evG_rear_fog_lights_Off</WidgetCallback>
                    </InputWidgetMapping>
                </ImageWidget>
                <ImageWidget var="img_2ndrow_radio_btn_divider">
                    <ID>img_rear_fog_lights_on_divider</ID>
