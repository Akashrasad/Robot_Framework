                        <DataValue>1</DataValue>
                        <StaticValue>virtualcontrols_1_4_0_6_FogRearOff_csm</StaticValue>
                        <Font var="var_font_radio_btn_off_normal"/>
                    </SelectorState>
                    <SelectorState>
                        <DataValue>2</DataValue>
                        <StaticValue>virtualcontrols_1_4_0_6_FogRearOff_csm</StaticValue>
