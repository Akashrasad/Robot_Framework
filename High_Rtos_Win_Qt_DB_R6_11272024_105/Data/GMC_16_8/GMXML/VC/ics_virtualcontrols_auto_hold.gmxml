                <LabelWidget var="var_action_bar_text_vc">
                    <ID>virtualcontrols_1_3_h_hold_csm</ID>
                </LabelWidget>
                <ImageWidget var="img_exit_btn">
                    <ID>img_auto_hold_close</ID>
